@{
    ViewData["Title"] = "Visualizar Atendimento";
    var nomeBarbeiro = ViewData["NomeBarbeiro"];
    var sobrenomeBarbeiro = ViewData["SobrenomeBarbeiro"];
    var celularBarbeiro = ViewData["CelularBarbeiro"];
    var dataHoraAtendimento = ViewData["DataHoraAtendimento"];
    var nomeCliente = ViewData["NomeCliente"];
    var sobrenomeCliente = ViewData["SobrenomeCliente"];
    var celularCliente = ViewData["CelularCliente"];
}

<div class="modal fade" tabindex="-1" role="dialog" id="visualizarAtendimentoModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Visualizar Atendimento</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p>Nome do Barbeiro: @nomeBarbeiro @sobrenomeBarbeiro</p>
                <p>Celular do Barbeiro: @celularBarbeiro</p>
                <p>Data e Hora do Atendimento: @dataHoraAtendimento</p>
                <p>Nome do Cliente: @nomeCliente @sobrenomeCliente</p>
                <p>Celular do Cliente: @celularCliente</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Voltar</button>
            </div>
        </div>
    </div>
</div>
